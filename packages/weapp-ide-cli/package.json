  "devDependencies": {
    "@icebreakers/rollup": "*",
    "@icebreakers/tsconfig": "*"
  },
  "dependencies": {
    "execa": "5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/utils.git"
  },
  "bugs": {
